<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar id="menu" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MenuController">
	<menus>
		<Menu mnemonicParsing="false" text="Artikli">
			<items>
				<Menu mnemonicParsing="false" text="Automobili">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#prikaziPretraguAutomobila" text="Pretraga" />
                  <MenuItem mnemonicParsing="false" onAction="#unosAutomobila" text="Unos" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Stanovi">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#prikaziPretraguStanova" text="Pretraga" />
                  <MenuItem mnemonicParsing="false" onAction="#unosStanova" text="Unos" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Usluge">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#prikaziPretraguUsluga" text="Pretraga" />
                  <MenuItem mnemonicParsing="false" onAction="#unosUsluga" text="Unos" />
					</items>
				</Menu>
			</items>
		</Menu>
		<Menu mnemonicParsing="false" text="Korisnici">
			<items>
				<Menu mnemonicParsing="false" text="Privatni korisnici">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#prikaziPretraguPrivatnihKorisnika" text="Pretraga" />
                  <MenuItem mnemonicParsing="false" onAction="#unosPrivatnihKorisnika" text="Unos" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Poslovni korisnici">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#prikaziPretraguPoslovnihKorisnika" text="Pretraga" />
                  <MenuItem mnemonicParsing="false" onAction="#unosPoslovnihKorisnika" text="Unos" />
					</items>
				</Menu>
			</items>
		</Menu>
      <Menu mnemonicParsing="false" text="Prodaje">
        <items>
            <MenuItem mnemonicParsing="false" onAction="#prikaziPretraguProdaja" text="Pretraga" />
            <MenuItem mnemonicParsing="false" onAction="#unosProdaja" text="Unos" />
        </items>
      </Menu>
	</menus>
</MenuBar>
